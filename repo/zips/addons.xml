        <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">KoalityBuild Wizard</summary>
    <description lang="en">Josh's krasses Family and Friends Build</description>
	<website>https://github.com/Joshless</website>
