<?xml version='1.0' encoding='utf-8'?>
<addons><addon id="koality" name="Koality Repo" version="1.0.1" provider-name="Joshless">
    <extension point="xbmc.addon.repository" name="koality">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/joshless/koality/master/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/joshless/koality/master/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/joshless/koality/master/repo/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Koality</summary>
        <description>Koality Build Repo</description>
        <disclaimer />
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon><addon id="plugin.program.koalitywizard" name="[COLOR limegreen]KoalityBuild[/COLOR]Wizard" version="1.0.0" provider-name="Joshless">
    <requires>
        <import addon="xbmc.python" version="2.1.0" />
        <import addon="script.module.requests" />
        <import addon="script.module.six" />
        <import addon="script.kodi.android.update" optional="true" version="1.1.7" />
        <import addon="script.speedtester" optional="true" version="1.1.2" />
    </requires>
    
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
    </extension>
    
    <extension point="xbmc.service" library="startup.py" start="startup" />
    
    <extension point="xbmc.addon.metadata">
        <summary lang="en">[COLOR limegreen]KoalityBuild[/COLOR]Wizard</summary>
        <description lang="en">[COLOR limegreen]Open[/COLOR]Josh's krasses Family and Friends Build.</description>
        <platform>all</platform>
        <reuselanguageinvoker>false</reuselanguageinvoker>
        <source>https://joshless.github.io/koality/</source>
        <news>
1.0.0
Release.
        </news>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.png</fanart>
        </assets>
    </extension>
</addon><addon id="plugin.video.fenlight" name="Fen Light" provider-name="Tikipeter" version="1.0.46">
    <requires>
        <import addon="xbmc.python" version="3.0.0" />
        <import addon="script.module.requests" version="2.19.1" />
    </requires>
    <extension library="resources/lib/fenlight.py" point="xbmc.python.pluginsource">
        <provides>video</provides>
    </extension>
    <extension library="resources/lib/service.py" point="xbmc.service" />
    <extension library="resources/lib/" point="xbmc.python.module" />
    <extension point="xbmc.addon.metadata">
        <reuselanguageinvoker>true</reuselanguageinvoker>
        <summary lang="en">Access your content from Easynews and Debrid Clouds.</summary>
        <description lang="en">This add-on is intended for advanced Kodi users only. You must own the content of any media you play through this addon. This is an English language only add-on.</description>
        <platform>all</platform>
		<disclaimer lang="en">The author of this addon does not host any of the content which is found and has no affiliation with any of the content providers.</disclaimer>
        <license />
        <assets>
            <icon>resources/media/fenlight_icon.png</icon>
            <fanart>resources/media/fenlight_fanart2.jpg</fanart>
        </assets>
        <news>INFO: For latest changes see Changelog within Tools of add-on.</news>
    </extension>
</addon></addons>